slas2.Pitch = 0.5
slas2:Play()
  lba.Parent = char
   lba.Material = "Neon"
    lba.BrickColor = BrickColor.new("Persimmon")
     lba.CanCollide = false
     lba.Shape = "Ball"
     lba.CFrame = handle.CFrame
     lba.Anchored = true
