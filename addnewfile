function DeleteParent(parent)
	for i,v in pairs(parent:children()) do
    if v:IsA("Part") then
        v:Destroy()
    end
end	
end

function do4()
		attack = true
		hum.WalkSpeed = 0
		local van = 1
			 local lba = Instance.new("Part")
				local slas = Instance.new("Sound",lba)
