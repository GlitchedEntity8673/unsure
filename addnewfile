					Effect='ResizeAndFade',
					Frames=45,
					Size=V3.N(.5,.5,.5),
					CFrame=CF.N(tor.CFrame.p),
					Mesh ={MeshType=Enum.MeshType.Sphere},
					Color=BrickColor.new'Really red',
					FXSettings={
