			hum.WalkSpeed = 0
			for i = 0, .2, 0.01 do
				NK.C0 = NK.C0:lerp(nkc0 * CFrame.Angles(math.rad(50),0, 0), i)
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
			   RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(180), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(180), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i)
			   LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i) 
				wait()
			end
			Orb.Anchored = true
			Orb.CanCollide = false
			OrbMesh.MeshType = Enum.MeshType.Sphere
			Orb.Size = Vector3.new(1,1,1)
			Orb.Material = Enum.Material.Neon
