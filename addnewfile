local Switcher = coroutine.wrap(function()
while true do
for i = 1, 50 do
CorruptedArm2.Transparency = CorruptedArm2.Transparency - 0.005
CorruptedArm.Transparency = CorruptedArm.Transparency + 0.02
wait()
end
for i = 1, 50 do
CorruptedArm2.Transparency = CorruptedArm2.Transparency + 0.005
CorruptedArm.Transparency = CorruptedArm.Transparency - 0.02
wait()
end
wait()
end
end)
