\32\61\32\72\105\116\98\111\120\46\84\111\117\99\104\101\100\58\99\111\110\110\101\99\116\40\102\117\110\99\116\105\111\110\40\104\105\116\41\32\68\97\109\97\103\101\40\104\105\116\44\109\97\116\104\46\114\97\110\100\111\109\40\56\44\49\52\41\44\51\41\32\101\110\100\41\10\9\80\108\97\121\83\111\117\110\100\40\67\108\97\119\44\49\44\49\46\50\44\83\111\117\110\100\115\91\49\93\41\10\9\102\111\114\32\105\32\61\32\49\44\49\48\32\100\111\10\9\9\108\101\114\112\40\87\76\83\44\87\76\83\46\67\48\44\76\83\67\48\32\42\67\70\114\97\109\101\46\65\110\103\108\101\115\40\48\44\48\44\45\109\97\116\104\46\112\105\47\56\41\44\97\114\109\115\112\101\101\100\42\49\46\51\41\10\9\9\108\101\114\112\40\87\82\83\44\87\82\83\46\67\48\44\82\83\67\48\32\42\67\70\114\97\109\101\46\65\110\103\108\101\115\40\109\97\116\104\46\112\105\47\49\46\52\44\45\109\97\116\104\46\112\105\47\53\44\45\109\97\116\104\46\112\105\47\53\41\44\97\114\109\115\112\101\101\100\42\49\46\51\41\10\9\9\108\101\114\112\40\87\76\72\44\87\76\72\46\67\48\44\76\72\67\48\44\108\101\103\115\112\101\101\100\41\10\9\9\108\101\114\112\40\87\82\72\44\87\82\72\46\67\48\44\82\72\67\48\44\108\101\103\115\112\101\101\100\41\10\9\9\108\101\114\112\40\87\82\74\44\87\82\74\46\67\48\44\82\74\67\48\32\42\67\70\114\97\109\101\46\65\110\103\108\101\115\40\48\44\48\44\109\97\116\104\46\112\105\47\53\41\44\115\112\101\101\100\41\10\9\9\108\101\114\112\40\78\101\99\107\44\78\101\99\107\46\67\48\44\78\67\48\32\42\67\70\114\97\109\101\46\65\110\103\108\101\115\40\48\44\48\44\45\109\97\116\104\46\112\105\47\53\41\44\115\112\101\101\100\41\10\9\9\105\102\32\86\101\99\116\111\114\51\46\110\101\119\40\72\77\46\86\101\108\111\99\105\116\121\46\88\44\48\44\72\77\46\86\101\108\111\99\105\116\121\46\90\41\46\109\97\103\110\105\116\117\100\101\32\62\61\32\50\32\116\104\101\110\10\9\9\9\108\101\114\112\40\87\76\72\44\87\76\72\46\67\48\44\76\72\67\48\32\42\67\70\114\97\109\101\46\65\110\103\108\101\115\40\48\44\48\44\109\97\116\104\46\115\105\110\40\108\101