				local EFSphere3 = CreateSphere()
				EffectSphereMesh.MeshId = "rbxassetid://20329976"
				EffectSphereMeshPart.Transparency = 1
				EffectSphereMeshPart.Size = Vector3.new(0.05,0.05,0.05)
				EffectSphereMeshPart.Anchored = true
				EffectSphereMeshPart.CanCollide = false
				EffectSphereMesh.Scale = Vector3.new(8,0,8)
				EffectSphereMesh.Offset = Vector3.new(0, 0, -0.6)
				ModelSphere.Name = "Sphere"
				ModelSphere.PrimaryPart = Sphere
				Sphere.Parent = ModelSphere
				Sphere.Anchored = true
				Sphere.CanCollide = false
				Sphere.Size = Vector3.new(0.05,0.05,0.05)
				Sphere.BrickColor = ManThemeColor
				Sphere.Material = Enum.Material.Neon
				EFSphere1.Parent = ModelSphere
				EFSphere1.Anchored = true
				EFSphere1.Transparency = 0.6
				EFSphere1.CanCollide = false
				EFSphere1.Size = Vector3.new(0.2,0.2,0.2)
				EFSphere1.BrickColor = ManThemeColor
				EFSphere1.Material = Enum.Material.Neon
				EFSphere2.Parent = ModelSphere
				EFSphere2.Anchored = true
				EFSphere2.Transparency = 0.6
				EFSphere2.CanCollide = false
