			tr1.Attachment0 = A
			tr1.Attachment1 = B
			tr1.Enabled = true
			tr1.Lifetime = LIFETIME
			tr1.TextureMode = "Static"
			tr1.LightInfluence = 0
			tr1.Color = COLOR
			tr1.Transparency = NumberSequence.new(0, 1)
			end
			
