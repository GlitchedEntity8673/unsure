heavyhitimpact.SoundId = "rbxassetid://541909913"
heavyhitimpact.Looped = false
heavyhitimpact:Play()
removeuseless:AddItem(heavyhitimpact,2)
h.Health = h.Health - 35
wait(1)
hit = false
end
end
