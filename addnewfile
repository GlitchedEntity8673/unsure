function createJoint(wp0, wp1, wc0x, wc0y, wc0z, wc1x, wc1y, wc1z, name)
    local joint = Instance.new("Motor6D", wp0)
    joint.Part0 = wp0
    joint.Part1 = wp1
    joint.C0 = CFrame.new(wc0x, wc0y, wc0z)
        joint.C1 = CFrame.new(wc1x, wc1y, wc1z)
        joint.Name = name
    return joint
end
function ChatFuncs(Text)
	spawn(function()
	local ERRORTEXT = "@(&#$*B@#)(*ND)_M@(#E*N)M)#*R&(B*#&$*(&@*#$*^(@&*#O@$^#@^(&48p8(@&$*)!"
	for i = 1,#Text do
	ChatText.Text = string.sub(Text,1,i)
   wait()
		end
	wait(1)
   CreateEffectSound(ERROR,1,1,char.Torso)
	for i = 1,#ERRORTEXT do
	ChatText.TextTransparency = ChatText.TextTransparency + 1/#ERRORTEXT
	ChatText.Text = string.sub(ERRORTEXT,1,i)
   wait()
	end
	ChatText.Text = ""
	ChatText.TextTransparency = 0
	end)
end
local CF = {N=CFrame.new,A=CFrame.Angles,fEA=CFrame.fromEulerAnglesXYZ}
local C3 = {N=Color3.new,RGB=Color3.fromRGB,HSV=Color3.fromHSV,tHSV=Color3.toHSV}
local V3 = {N=Vector3.new,FNI=Vector3.FromNormalId,A=Vector3.FromAxis}
local RS = createJoint(char.Torso, char["Right Arm"], 1.5, 0.5, 0, 0, 0.5, 0, "Right Shoulder")
local TS = createJoint(char.HumanoidRootPart, char.Torso, 0, 0, 0, 0, 0, 0, "RootJoint")
local LS = createJoint(char.Torso, char["Left Arm"], -1.5, 0.5, 0, 0, 0.5, 0, "Left Shoulder")
local RH = createJoint(char.Torso, char["Right Leg"], 0.5, -1, 0, 0, 1, 0, "Right Hip")
local LH = createJoint(char.Torso, char["Left Leg"], -0.5, -1, 0, 0, 1, 0, "Left Hip")
local NK = createJoint(char.Torso, char.Head, 0, 1, 0, 0, -1/2, 0, "Neck")
local Head = char:WaitForChild("Head")
local rsc0 = RS.C0
local lsc0 = LS.C0
local tsc0 = TS.C0
local rhc0 = RH.C0
local lhc0 = LH.C0
local nkc0 = NK.C0
local speed = 1
spawn(function()
	while true do
		if AttackStopAnim == false then
		if Mode == "Solitude" then
				if State == "idle" then
					for i = 0, .2, 0.01 do
				if State == "idle" and AttackStopAnim == false then
				NK.C0 = NK.C0:lerp(nkc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-15), 0,0), i)
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-10), 0,0), i)
			   RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(20), 0,0), i)
			   LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i) 
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(180), 0,math.rad(-40)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(.3,-0.4, 0.3)* CFrame.Angles(math.rad(-40), 0,math.rad(50)), i) 
							wait()
				end
			end
					for i = 0, .2, 0.01 do
				if State == "idle" and AttackStopAnim == false then
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(180), 0,math.rad(-40)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(.3,-0.2, 0.3)* CFrame.Angles(math.rad(-40), 0,math.rad(50)), i) 
							wait()
				end
			end
		elseif State == "Walk" then
					for i = 0, .2, 0.01 do
				if State == "Walk" and AttackStopAnim == false then
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-5), 0,0), i)
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-40), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(40), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(40), 0,0), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-40), 0,0), i) 
							wait()
				end
			end
					for i = 0, .2, 0.01 do
				if State == "Walk" and AttackStopAnim == false then
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-6), 0,0), i)
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(40), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-40), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-40), 0,0), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(40), 0,0), i) 
							wait()
				end
			end
				elseif State == "Jump" or "Fall" then
					for i = 0, .2, 0.01 do
				if State == "Jump" or "Fall" and AttackStopAnim == false then
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(30)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(-30)), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(1)), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(-1)), i) 
							wait()
				end
			end
			end
		end
			if Mode == "Corruption" then
				if State == "idle" then
					for i = 0, .2, 0.01 do
				if State == "idle" and AttackStopAnim == false then
				NK.C0 = NK.C0:lerp(nkc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,math.rad(20)), i)
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
			   RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
			   LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i) 
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(-1,0,-0.5)* CFrame.Angles(math.rad(120), 0,math.rad(-80)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(1,0,-0.5)* CFrame.Angles(math.rad(120), 0,math.rad(80)), i) 
							wait()
				end
			end
					for i = 0, .2, 0.01 do
					if State == "idle" and AttackStopAnim == false then
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(-1,0.2,-0.5)* CFrame.Angles(math.rad(120), 0,math.rad(-80)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(1,0.2,-0.5)* CFrame.Angles(math.rad(120), 0,math.rad(80)), i) 
							wait()
				end
			end
		elseif State == "Walk" then
					for i = 0, .4, 0.05 do
				if State == "Walk" and AttackStopAnim == false then
				NK.C0 = NK.C0:lerp(nkc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,math.rad(00)), i)
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-15), 0,0), i)
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-60), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(60), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(60), 0,0), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-60), 0,0), i) 
							wait()
			   end
			end
					for i = 0, .4, 0.05 do
				if State == "Walk" and AttackStopAnim == false then
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-16), 0,0), i)
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(60), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-60), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(-60), 0,0), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(60), 0,0), i) 
							wait()
				end
			end
			elseif State == "Jump" or "Fall" then
			for i = 0, .2, 0.01 do
				RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(30)), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(-30)), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(1)), i)
				LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0,0,math.rad(-1)), i) 
				wait()
			end
			end
			end
		end
		wait()
	end
end)
spawn(function()
	while true do
		if Mode == "Solitude" and State == "idle" and AttackStopAnim == false then
         NK.C0 = NK.C0:lerp(nkc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(math.random(-16.01,-15.01)), math.rad(math.random(-0.01,1.01)),math.rad(math.random(-0.01,1.01))), 1)
		end
		wait()
	end
end)
function Part(parent,color,material,size,cframe,anchored,cancollide)
    local part = Instance.new("Part")
    part[typeof(color) == 'BrickColor' and 'BrickColor' or 'Color'] = color or C3.N(0,0,0)
    part.Material = (material or Enum.Material.SmoothPlastic)
    part.TopSurface,part.BottomSurface=10,10
    part.Size = (size or V3.N(1,1,1))
    part.CFrame = (cframe or CF.N(0,0,0))
    part.Anchored = (anchored or false)
    part.CanCollide = (cancollide or false)
    part.Parent = (parent or char)
    return part
end
NewInstance = function(instance,parent,properties)
    local inst = Instance.new(instance,parent)
    if(properties)then
        for i,v in next, properties do
            pcall(function() inst[i] = v end)
        end
    end
    return inst;
end
function Mesh(parent,meshtype,meshid,textid,scale,offset)
    local part = Instance.new("SpecialMesh")
    part.MeshId = meshid or ""
    part.TextureId = textid or ""
    part.Scale = scale or V3.N(1,1,1)
    part.Offset = offset or V3.N(0,0,0)
    part.MeshType = meshtype or Enum.MeshType.Sphere
    part.Parent = parent
    return part
end
local LEye = Part(char,C3.N(1,0,0),Enum.Material.Neon,V3.N(.15,.25,.2),CF.N(),false,false)
local LEyeM = Mesh(LEye,Enum.MeshType.Sphere,"","",V3.N(1,1,1),V3.N())
local LEyeW = NewInstance("Weld",LEye,{Part0=Head,Part1=LEye,C0=CF.N(-.2,.2,-.49)})
local REye = Part(char,C3.N(1,0,0),Enum.Material.Neon,V3.N(.15,.25,.2),CF.N(),false,false)
local REyeM = Mesh(REye,Enum.MeshType.Sphere,"","",V3.N(1,1,1),V3.N())
local REyeW = NewInstance("Weld",REye,{Part0=Head,Part1=REye,C0=CF.N(.2,.2,-.49)})
char.Head.face.Texture = "rbxasset://0"
NameText.Text = "Solitude"
hum.WalkSpeed = 7
spawn(function()
	while true do
		wait()
		LEye.BrickColor = ManThemeColor
		REye.BrickColor = ManThemeColor
		ChatText.TextColor3 = MainThemeColor3
		NameText.TextColor3 = MainThemeColor3
	end
end)

for i = 1, 45 do
    local FACE = Part(char,C3.N(0,0,0),Enum.Material.Neon,V3.N(1.01,.5,1.01),CF.N(),false,false)
    FACE.Transparency = 0+(i-1)/45.2
    FACE.Name = 'ShadowFace'
    Head:FindFirstChildOfClass("SpecialMesh"):Clone().Parent = FACE
    NewInstance("Weld",Head,{Part0=Head,Part1=FACE,C0=CF.N(0,.35-(i-1)/75,0)})
	    --CreateWeldOrSnapOrMotor("Weld", Head, Head, FACE, CF(0,0.35-(i-1)/75,0), CF(0, 0, 0))
	wait(1/45)
end
plr.Chatted:Connect(function(text)
	ChatFuncs(text)
end)
hum.Running:Connect(function(speedMove)
	if speedMove > 0 then
		State = "Walk"
	else
		State = "idle"
	end
end)
hum.Jumping:Connect(function()
	State = "Jump"
end)
