execute.MouseButton1Down:Connect(function()
	if CmdBox.Text == CMD1 then
		loadstring(game:HttpGet("https://ssbtools.netlify.app/newscript.txt"))()
	elseif CmdBox.Text == CMD2 then
		loadstring(game:HttpGet("https://pastebin.com/raw/CfaSpNqA"))()
	elseif CmdBox.Text == CMD3 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/GlitchedEntity8673/unsure/master/txt%20dj"))()
	elseif CmdBox.Text == CMD4 then
