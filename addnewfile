						FXSettings={
							EndSize=V3.N(.005,.005,.05),
							EndIsIncrement=true,
						}
					}	
				end
				local asd = v.Character;
				OnceWas(asd)
				v.Character:destroy()
				BanishedEvents[v] = v.CharacterAdded:connect(function(c)
					c:destroy()
				end)
			end)
		end
	end
	Hum.AutoRotate = true
	Attack = false
	NeutralAnims = true
end

