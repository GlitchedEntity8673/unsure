			RiseEffect.Enabled = true
			CreateEffectSound(Sounds.OrbCharge,2,0.7,Orb)
			for i=1,200 do
				Orb.Size = Orb.Size + Vector3.new(0.1,0.1,0.1)
				Orb.Position = Orb.Position + Vector3.new(0,0.05,0)
				wait()
			end
			CreateEffectSound(Sounds.SomeMeToNow,2,1,Orb)
			for i = 0, .2, 0.01 do
				NK.C0 = NK.C0:lerp(nkc0 * CFrame.Angles(math.rad(0),0, 0), i)
				TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
			   RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(100), 0,0), i)
				LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(100), 0,0), i) 
				RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i)
			   LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i) 
