	CmdBox.Text = ""
end)
clrframe.Name = "clrframe"
clrframe.Parent = clear
clrframe.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
clrframe.BorderSizePixel = 0
clrframe.Position = UDim2.new(0, 0, 1, 0)
clrframe.Size = UDim2.new(0, 207, 0, 3)

-- Scripts:

local function DDBV_fake_script() -- execframe.RainbowFunction 
	local script = Instance.new('LocalScript', execframe)

	while true do 
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(i, 0, n)
	wait() 
	end
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(n, i, 0)
	wait()
	end
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(0, n, i)
	wait() 
	end 
	end
end
coroutine.wrap(DDBV_fake_script)()
local function GDLBR_fake_script() -- clrframe.RainbowFunction 
	local script = Instance.new('LocalScript', clrframe)

	while true do 
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(i, 0, n)
	wait() 
	end
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(n, i, 0)
	wait()
	end
	i = 0
	for n=1,0,-0.05 do 
	i = i + 0.05
	script.Parent.BackgroundColor3 = Color3.new(0, n, i)
	wait() 
	end 
	end
end
coroutine.wrap(GDLBR_fake_script)()
