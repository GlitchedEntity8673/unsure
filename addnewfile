				EFSphere2.Material = Enum.Material.Neon
				EFSphere3.Transparency = 0.6
				EFSphere3.Parent = ModelSphere
				EFSphere3.Anchored = true
				EFSphere3.CanCollide = false
				EFSphere3.Size = Vector3.new(0.10,0.10,0.10)
				EFSphere3.BrickColor = ManThemeColor
				EFSphere3.Material = Enum.Material.Neon
				hum.WalkSpeed = 0
				for i = 0, .2, 0.01 do
					NK.C0 = NK.C0:lerp(nkc0 * CFrame.Angles(math.rad(0),0, 0), i)
					TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
					RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(180), 0,0), i)
					LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i) 
					RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i)
					LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i) 
					wait()
				end
				ModelSphere.Parent = EffectsFolder
				Sphere.Position = char:WaitForChild("Right Arm").Position
				EFSphere1.Position = char:WaitForChild("Right Arm").Position
				EFSphere2.Position = char:WaitForChild("Right Arm").Position
				EFSphere3.Position = char:WaitForChild("Right Arm").Position
				Sphere.Position = Sphere.Position + Vector3.new(0,2,0)
				EFSphere1.Position = EFSphere1.Position + Vector3.new(0,2,0)
				EFSphere2.Position = EFSphere1.Position + Vector3.new(0,2,0)
				EFSphere3.Position = EFSphere1.Position + Vector3.new(0,2,0)
				CreateEffectSound(Sounds.OrbCharge,1,1,Sphere)
				for i=1,50 do
					Sphere.Size = Sphere.Size + Vector3.new(0.1,0.1,0.1)
					Sphere.Position = Sphere.Position + Vector3.new(0,0.05,0)
					EFSphere1.Size = EFSphere1.Size + Vector3.new(0.1,0.1,0.1)
					EFSphere1.Position = EFSphere1.Position + Vector3.new(0,0.05,0)
					EFSphere2.Size = EFSphere1.Size + Vector3.new(0.1,0.1,0.1)
					EFSphere2.Position = EFSphere1.Position + Vector3.new(0,0.05,0)
					EFSphere3.Size = EFSphere1.Size + Vector3.new(0.1,0.1,0.1)
					EFSphere3.Position = EFSphere1.Position + Vector3.new(0,0.05,0)
					wait(0)
				end
				for i = 0, .2, 0.01 do
					NK.C0 = NK.C0:lerp(nkc0 * CFrame.Angles(math.rad(0),0, 0), i)
					TS.C0 = TS.C0:lerp(tsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i)
					RS.C0 = RS.C0:lerp(rsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(100), 0,0), i)
					LS.C0 = LS.C0:lerp(lsc0 * CFrame.new(0,0,0)* CFrame.Angles(math.rad(0), 0,0), i) 
					RH.C0 = RH.C0:lerp(rhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i)
					LH.C0 = LH.C0:lerp(lhc0 * CFrame.new(0,0,0)* CFrame.Angles(0, 0,0), i) 
					wait()
				end
				TweenModel(ModelSphere,Mouse.Hit,5,Enum.EasingDirection.Out,Enum.EasingStyle.Quint)
				spawn(function()
					local EffectSM2 = EffectSphereMeshPart:Clone()
					local EffectSM3 = EffectSphereMeshPart:Clone()
				   Sphere.Touched:Connect(function(PathSS)
					   if not PathSS.Parent:FindFirstChild("IsHit") then
						   dmg(PathSS.Parent)
					   end
				   end)
					wait(5)
					MultiTween(EffectSphereMeshPart,"Transparency",0,0.3)
					MultiTween(EffectSM2,"Transparency",0,0.3)
					MultiTween(EffectSM3,"Transparency",0,0.3)
					CreateEffectSound(Sounds.Explosion,1,1,Sphere)
					EffectSphereMeshPart.Position = Sphere.Position
					for i=1,30 do
						Sphere.Size = Sphere.Size + Vector3.new(2,2,2)
						EFSphere1.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EFSphere2.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EFSphere3.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EffectSphereMesh.Scale = EffectSphereMesh.Scale + Vector3.new(2,0,2)
						EffectSphereMeshPart.Orientation = EffectSphereMeshPart.Orientation + Vector3.new(2,2,2)
						EffectSM2.Mesh.Scale = EffectSM2.Mesh.Scale + Vector3.new(2,0,2)
						EffectSM2.Orientation = EffectSM2.Orientation + Vector3.new(4,4,4)
						EffectSM3.Mesh.Scale = EffectSM3.Mesh.Scale + Vector3.new(2,0,2)
						EffectSM3.Orientation = EffectSM3.Orientation + Vector3.new(6,6,6)
						wait()
					end
					MultiTween(EFSphere1,"Transparency",1,1)
					MultiTween(EFSphere2,"Transparency",1,1)
					MultiTween(EFSphere3,"Transparency",1,1)
					MultiTween(Sphere,"Transparency",1,1)
					MultiTween(EffectSphereMeshPart,"Transparency",1,1)
					MultiTween(EffectSM2,"Transparency",1,1)
					MultiTween(EffectSM3,"Transparency",1,1)
					for i=1,10 do
						Sphere.Size = Sphere.Size + Vector3.new(2,2,2)
						EFSphere1.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EFSphere2.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EFSphere3.Size = EFSphere1.Size + Vector3.new(2,2,2)
						EffectSphereMesh.Scale = EffectSphereMesh.Scale + Vector3.new(2,0,2)
						EffectSphereMeshPart.Orientation = EffectSphereMeshPart.Orientation + Vector3.new(2,2,2)
						EffectSM2.Mesh.Scale = EffectSM2.Mesh.Scale + Vector3.new(2,0,2)
						EffectSM2.Orientation = EffectSM2.Orientation + Vector3.new(2,2,2)
						EffectSM3.Mesh.Scale = EffectSM3.Mesh.Scale + Vector3.new(2,0,2)
						EffectSM3.Orientation = EffectSM3.Orientation + Vector3.new(2,2,2)
						wait()
					end
					wait(0.5)
					ModelSphere:Destroy()
				end)
				AttackStopAnim = false
				Attack = false
				hum.WalkSpeed = 40
			end
		end
	end
	print(key)
end)
Mouse.KeyDown:connect(function(key)
	print("Mode Change")
		if key == "e" and Mode ~= "Corruption" and AttackStopAnim == false and Attack == false then
		ManThemeColor = BrickColor.new("Royal purple")
		MainThemeColor3 = Color3.fromRGB(170, 85, 255)		
		Mode = "Corruption"
		NameText.Text = "Corruption"
		song.SoundId = Sounds.CorruptionOst
		hum.WalkSpeed = 40
	end
	if key == "q" and Mode ~="Solitude" and AttackStopAnim == false and Attack == false then
		Attack = true
		local Sphere = Instance.new("Part")
		local SphereMesh = Instance.new("SpecialMesh",Sphere)
		Sphere.Position = char.Torso.Position
		Sphere.Size = Vector3.new(0,0,0)
		Sphere.BrickColor = BrickColor.new("Really red")
		SphereMesh.MeshType = Enum.MeshType.Sphere
		Sphere.Material = Enum.Material.Neon
		Sphere.CanCollide = false
		Sphere.Anchored = true
		Sphere.Parent = EffectsFolder
		hum.WalkSpeed = 0
		MultiTween(Sphere,"Transparency",1,1.5)
		for i=1,50 do
			Sphere.Size = Sphere.Size + Vector3.new(1,1,1)
			wait()
		end
		Sphere.BrickColor = ManThemeColor
		MultiTween(Sphere,"Transparency",0,1)
		for i=1,30 do
			Sphere.Size = Sphere.Size - Vector3.new(1,1,1)
			wait()
		end
		ManThemeColor = BrickColor.new("Really red")
		MainThemeColor3 = Color3.fromRGB(255,0,0)
